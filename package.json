{
  "name": "tailwind-css",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.9",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "latest",
    "axios": "^1.1.3",
    "joi": "^17.6.3",
    "lodash": "^4.17.21",
    "material-react-table": "^1.2.9",
    "moment": "^2.29.4",
    "postcss": "^8.4.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.6.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.0.8",
    "recharts": "^2.1.15",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "cp ./.env.dev .env && react-scripts start",
    "start:test": "cp ./.env.test .env && react-scripts start",
    "start:prod": "cp ./.env.prod .env && react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:dev": "cp ./.env.dev .env && GENERATE_SOURCEMAP=false react-scripts build",
    "build:test": "cp ./.env.test .env && GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "cp ./.env.prod .env && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e2e-test": "cypress open",
    "lint": "eslint 'src/**/*.{js,jsx}' && stylelint src/**/*.css",
    "fix": "eslint 'src/**/*.{js,jsx}' --fix && stylelint src/**/*.css --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc.json ",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prepare": "husky install",
    "coverage": "react-scripts test --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "axios-mock-adapter": "^1.21.2",
    "cypress": "^10.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0"
  }
}
